{
  "name": "sprint_1",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "rm -rf dist/* && parcel src/*.html --no-cache --no-source-maps",
    "build": "rm -rf dist/* && parcel build src/*.html --target app --dist-dir dist --public-url ./ --no-source-maps --no-cache --no-optimize",
    "start": "npm run build && node server.js"
  },
  "engines": {
    "node": ">=12"
  },
  "targets": {
    "app": {
      "context": "browser",
      "includeNodeModules": true,
      "outputFormat": "esmodule"
    }
  },
  "author": "Ivan Kuznetsov",
  "license": "ISC",
  "dependencies": {
    "handlebars": "^4.7.7"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.0.1",
    "@parcel/transformer-sass": "^2.0.1",
    "autoprefixer": "^10.4.0",
    "express": "^4.17.1",
    "parcel": "^2.0.1",
    "parcel-transformer-hbs": "^1.0.4",
    "postcss": "^8.4.1",
    "sass": "^1.43.5"
  }
}
